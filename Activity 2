fullname = "Avril Satu Celesios"
age = 18
height = 1.64
currently_working = False

print("Fullname:", fullname)
print(type(fullname))

print("\nAge:", age)
print(type(age))

print("\nHeight in meters:", height)
print(type(height))

print("\nCurrently working?", currently_working)
print(type(currently_working))
